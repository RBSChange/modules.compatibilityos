<?xml version="1.0" encoding="UTF-8"?>
<urlrewriting>
	<rules>
		<rule documentModel="privateshop" viewMode="detail">
			<template>/$label,$lang,$id</template>
		</rule>
		<rule documentModel="sale" viewMode="detail">
			<template>/vente-privee/$id/$label</template>
		</rule>
		<rule redirection="privatesales/GoToExternalSale">
			<template>/vente-externe/$cmpref.html</template>
			<parameters>
				<globalParameter name="cmpref" format="[0-9]+" />
			</parameters>
		</rule>
		<rule documentModel="privateshelf" viewMode="detail">
			<template>/vente-privee/$saleId/$saleLabel/$id/$label</template>
			<parameters>
				<parameter name="saleId" method="getSaleId" format="[0-9]+" />
				<parameter name="saleLabel" method="getSaleLabel" />
			</parameters>
		</rule>
		<rule documentModel="order" viewMode="detail">
			<template>/orderdetail/$id/$orderNumber</template>
			<parameters>
				<parameter name="orderNumber" method="getOrderNumber" />
			</parameters>
		</rule>
		<rule redirection="privatesales/ViewFeed">
			<template>/privatesales/feed/$cmpref/$label</template>
		</rule>
	</rules>
</urlrewriting>